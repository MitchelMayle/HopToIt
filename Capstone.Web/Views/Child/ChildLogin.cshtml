@using Capstone.Web.Models;
@model ChildModel

<!DOCTYPE html>

<h2>Child Login</h2>

<nav>
    <ul>
        <li>
            @Html.ActionLink("Home", "Index")
        </li>
    </ul>
</nav>
<body>
    <div>
        <label>User Name</label>
        @Html.TextBoxFor(m => m.UserName)
